  "repository": {
    "type": "git",
    "url": "git+https://github.com/WodenWang820118/nx-ng-nest-electron.git"
  },
  "author": "Guan Xin Wang",
  "description": "An Electron application built with various frameworks",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "development": "-------------all development scripts-------------",
    "dev-front": "nx serve ng-tracker",
    "dev-vue": "nx serve vue-tracker",
    "dev-react": "nx serve react-tracker",
    "dev-back": "cross-env-shell NODE_ENV=dev PORT=3000 \"nx serve nest-backend --watch\"",
    "dev-express": "cross-env-shell NODE_ENV=dev PORT=3000 \"nx serve express-backend --watch\"",
    "dev-spring": "cross-env-shell NODE_ENV=dev PORT=3000 \"nx serve spring-backend\"",
    "prod-back": "cross-env-shell NODE_ENV=prod PORT=5000 \"nx serve nest-backend --watch\"",
    "dev-electron": "cross-env-shell NODE_ENV=dev PORT=3000 \"pnpm run build && electron-forge start\"",
    "building scripts": "-----------------all building scripts-----------------",
    "build-front": "nx build ng-tracker --configuration=development --optimization --base-href ./",
    "build-back": "cross-env-shell NODE_ENV=dev PORT=3000 \"nx build nest-backend\"",
    "build-express": "cross-env-shell NODE_ENV=dev PORT=3000 \"nx build express-backend\"",
    "build-spring": "nx build spring-backend",
    "build": "pnpm run build-front && pnpm run build-back",
    "build-prod-front": "nx build ng-tracker --configuration production --optimization --base-href ./",
    "build-prod-back": "cross-env-shell NODE_ENV=prod PORT=5000 \"nx build nest-backend\"",
    "build-prod": "pnpm run build-prod-front && pnpm run build-prod-back && cd dist/nest-backend && npm install && npm install sqlite3 update-electron-app electron-log && cd ../..",
    "make": "cross-env-shell NODE_ENV=prod PORT=5000 \"pnpm run build-prod && electron-forge make\"",
    "package": "cross-env-shell NODE_ENV=prod PORT=5000 \"pnpm run build-prod && electron-forge package\"",
    "testing": "-----------------all testing scripts-----------------",
    "test-back:cov": "vitest --coverage --root=apps/nest-backend --exclude=**/test/*e2e-spec.ts --watch=false",
    "publishing": "-----------------publishing scripts-----------------",
    "publish-local": "cross-env-shell NODE_ENV=prod PORT=5000 \"pnpm run build-prod && electron-forge publish -- --from-dry-run\"",
    "publish-dry": "cross-env-shell NODE_ENV=prod PORT=5000 \"pnpm run build-prod && electron-forge publish -- --dry-run\"",
    "publish": "cross-env-shell NODE_ENV=prod PORT=5000 \"pnpm run build-prod && electron-forge publish\""
  },
  "private": true,
